{
  "name": "aws-utilities",
  "version": "1.0.0",
  "description": "This is a collection of simple command-line scripts, bash aliases, and bash utilities that make using the AWS CLI even easier.",
  "type": "module",
  "nnnnmain": "index.js",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "scripts": {
    "awsgenerate": "node ./lib/generate/commander.js",
    "precreate:dev": "npm run awsgenerate cognito",
    "preupdate:dev": "npm run awsgenerate cognito",
    "postdelete:dev": "rm -rf mysampleapp-dev-cloud-formation.json",
    "create:dev": "aws cloudformation create-stack --stack-name my-sample-app-dev --template-body file://mysampleapp-dev-cloud-formation.json",
    "delete:dev": "aws cloudformation delete-stack --stack-name my-sample-app-dev",
    "update:dev": "aws cloudformation update-stack --stack-name my-sample-app-dev --template-body file://mysampleapp-dev-cloud-formation.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkiel/aws-utilities.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rkiel/aws-utilities/issues"
  },
  "homepage": "https://github.com/rkiel/aws-utilities#readme",
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^9.4.0",
    "conf": "^10.1.2",
    "lodash": "^4.17.21"
  }
}
